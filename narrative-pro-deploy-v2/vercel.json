{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "src": "/auth/callback",
      "dest": "/auth/callback/index.html"
    },
    {
      "src": "/auth/callback/",
      "dest": "/auth/callback/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}